{

  "manifest_version": 2,
  "name": "Copy as PNG",
  "version": "1.0",

  "description": "Converts image to PNG before putting it on clipboard",

  "icons": {
    "48": "icon.png"
  },

  "permissions":[
      "<all_urls>",
      "contextMenus",
      "clipboardWrite"
  ],

  "background": {
      "scripts": ["background.js"]
  }

}
